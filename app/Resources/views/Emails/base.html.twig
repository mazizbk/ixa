<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>{% block subject %}{% endblock subject %}</title>
</head>
<body bgcolor="#F4F4F4">
<table>
    <tr>
        <td>
            <table class="top-container">
                <tr>
                    <td class="text-center"><a href="{{ url('azimut_frontoffice', {path: ''}) }}"><img src="{{ absolute_url(asset('img/frontoffice/logo-workcare.png')) }}" alt="Workcare" class="logo" /></a></td>
                </tr>
                <tr>
                    <td class="container" colspan="2">{% block body %}{% endblock body %}</td>
                </tr>
            </table>

        </td>
    </tr>
</table>
</body>
</html>
